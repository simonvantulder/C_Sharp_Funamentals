@using Hogwarts.Models
@* @model Survey *@

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/style.css">
    <title>Document</title>
</head>
<body>
    <h2>Home | <a href="/projects"> Projects</a> | <a href="/contact">Contact</a> </h2>

    <h1>Contact Me</h1>
    <div>
        @foreach(Survey student in Model.Survey)
        {
        <h3>Name: @student.Name</h3>
        <h3>Location: @student.Location</h3>
        <h3>Favorite Language: @student.Language</h3>
        <h3>Message: @student.Message</h3>
        }
    </div>
    


</body>



</html>